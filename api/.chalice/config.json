{
  "version": "2.0",
  "app_name": "program-rig-api",
  "stages": {
    "dev": {
      "api_gateway_stage": "api-dev",
      "api_gateway_custom_domain": {
        "domain_name": "pr-api-dev.helpfulowl.com",
        "tls_version": "TLS_1_2",
        "certificate_arn": "arn:aws:acm:us-east-1:490956688664:certificate/2d90b6f4-40e3-4181-ba28-1455c0e6159e"
      },
      "autogen_policy": false,
      "iam_policy_file": "app-policy.json",
      "log_retention_in_days": 1,
      "environment_variables": {
        "CACHE_TABLE_NAME": "program-rig-cache-dev",
        "CORS_ORIGIN": "*"
      }
    },
    "prod": {
      "api_gateway_stage": "api",
      "api_gateway_custom_domain": {
        "domain_name": "pr-api.helpfulowl.com",
        "tls_version": "TLS_1_2",
        "certificate_arn": "arn:aws:acm:us-east-1:490956688664:certificate/2d90b6f4-40e3-4181-ba28-1455c0e6159e"
      },
      "autogen_policy": false,
      "iam_policy_file": "app-policy.json",
      "log_retention_in_days": 14,
      "environment_variables": {
        "CACHE_TABLE_NAME": "program-rig-cache-prod",
        "CORS_ORIGIN": "https://pr.helpfulowl.com"
      }
    }
  }
}
